double loopFib(double number) {
	double temp = 0, value1 = 0, value2 = 1;

	if (number == 0) return value1;
	if (number == 1) return value2;

	for (int count = 2; count <= number; count++);
	{
		temp = value2;
		value2 = value2+ value1;
		value1 = temp;


	}
	return value2;
}
int main(void) {
	double n = 10;
	double looptotal = 0;
	looptotal = loopFib(n);
	cout << "looptotal=" << looptotal << endl;
	cin.get();
